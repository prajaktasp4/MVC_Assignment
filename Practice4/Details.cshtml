@model EntityFrameworkExample.Models.Product

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Details</h2>
<table class="table">
    <tr>
        <td>ProductID</td>
        <td>@Model.ProductID</td>
    </tr>
    <tr>
        <td>ProductName</td>
        <td>@Model.ProductName</td>
    </tr>
    <tr>
        <td>Price</td>
        <td>@Model.Price</td>
    </tr>
    <tr>
        <td>DateOfPurchase</td>
        <td>@Model.DateOfPurchase</td>
    </tr>
    <tr>
        <td>AvailabilityStatus</td>
        <td>@Model.AvailabilityStatus</td>
    </tr>
    <tr>
        <td>CatagoryID</td>
        <td>@Model.CatagoryID</td>
    </tr>
    <tr>
        <td>BrandID</td>
        <td>@Model.BrandID</td>
    </tr>
    <tr>
        <td>Active</td>
        <td>@Model.Active</td>
    </tr>
    
</table>

<a href="/product/Index">Back To Products</a>

